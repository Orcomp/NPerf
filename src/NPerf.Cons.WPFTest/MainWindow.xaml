<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="clr-namespace:OxyPlot.Wpf;assembly=OxyPlot.Wpf"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="NPerf.Cons.WPFTest.MainWindow"
        Title="NPerf Benchmark Test" Height="640" Width="600">
    <Grid Margin="0,0,252,40" x:Name="LayoutRoot">
        <ScrollViewer Margin="8,8,-243,28">
    	<Grid x:Name="ChartsGrid">    		
    		<Grid.ColumnDefinitions>
    			<ColumnDefinition />
    			<ColumnDefinition/>
    		</Grid.ColumnDefinitions>
    	</Grid>
        </ScrollViewer>
        <TextBox x:Name="OutputTextbox" Margin="8,0,-109,-36" TextWrapping="Wrap" VerticalAlignment="Bottom" Height="58" />
    	<Button Content="Start Tests" HorizontalAlignment="Right" Height="21.96" Margin="0,0,-243,-36" VerticalAlignment="Bottom" Width="128" Click="Button_Click"/>
        <RadioButton Content="Linear Y-Axis" HorizontalAlignment="Right" Height="18.04" Margin="0,0,-243,10" VerticalAlignment="Bottom" Width="128" GroupName="axis" IsChecked="True"/>
        <RadioButton x:Name="AxisParameter" Content="Logarithmic Y-Axis" HorizontalAlignment="Right" Height="18.04" Margin="0,0,-243,-10.04" VerticalAlignment="Bottom" Width="128" GroupName="axis"/>
    </Grid>
</Window>
